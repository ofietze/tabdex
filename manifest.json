{
  "manifest_version": 2,
  "name": "tabdex",
  "version": "1.1",
  "description": "Navigate through all open tabs easily with keyboard shortcuts",
  
  "permissions": [
    "tabs"
  ],
  
  "browser_action": {
    "default_title": "tabdex",
    "default_icon": {
      "48": "icon48.png",
      "96": "icon96.png",
      "128": "icon128.png"
    }
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  }
}
